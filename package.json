{
  "name": "react-speedrun-tools",
  "displayName": "React Speedrun Tools",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
      "command": "react-speedrun-tools.removeBoilerplate",
      "category": "React Speedrun Tools",
      "title": "Remove Boilerplate And Install Sass"
      },
      {
        "command": "react-speedrun-tools.createReactComponent",
        "category": "React Speedrun Tools",
        "title": "Create A New React Component"
      },
      {
        "command": "react-speedrun-tools.addFont",
        "category": "React Speedrun Tools",
        "title": "Add A Font"
      },
      {
        "command": "react-speedrun-tools.generateColorVariables",
        "category": "React Speedrun Tools",
        "title": "Generate Color Variables"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.3.0"
  }
}
